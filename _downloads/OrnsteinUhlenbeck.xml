  <im:driftDiffusivity nonLinear="true" computeMoments="false">
    <im:method><![CDATA[// set up an Ornstein-Uhlenbeck process\n"
All->DiffusivityX = diffX;
All->DiffusivityY = diffY;

All->DriftX = -(gxx*PhysicalX + gxy*PhysicalY);
All->DriftY = -(gyx*PhysicalX + gyy*PhysicalY);]]></im:method>
  </im:driftDiffusivity>
  <im:setField>
    <im:method><![CDATA[]]></im:method>
  </im:setField>
  <im:newIndividualsMethod>
    <im:method><![CDATA[]]></im:method>
  </im:newIndividualsMethod>
</im:sbml>
